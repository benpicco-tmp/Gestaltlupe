  <Entry Key="Intern.Formula.Source" Value="public override void Init() {&#xA;  base.Init();&#xA;  additionalPointInfo=new AdditionalPointInfo();        &#xA;}&#xA;&#xA;&#xA;// Mandelbox&#xA;        public override long InSet(double ar, double ai, double aj,  double br, double bi, double bj, double bk, long zkl, bool invers) {&#xA;&#xA; double gr=50000;&#xA; double xx, yy, zz;&#xA; long tw;&#xA; int n;&#xA; double x=ar,y=ai,z=aj;&#xA; double r_n = 0;&#xA;&#xA; xx = x * x; yy = y * y; zz = z * z; &#xA; tw = 0;&#xA; double r = Math.Sqrt(xx + yy + zz);&#xA; double scale = 1.05;&#xA; double fixedRadius = 1;&#xA; double mult=1;&#xA; double red=0;&#xA; double green=0;&#xA; double blue=0;&#xA;&#xA; for (n = 1; n &lt; zkl; n++) {&#xA;   scale=scale-ai*(scale+1);&#xA;&#xA;//  fold box onto itself  &#xA;  if (x &gt; 1)&#xA;    x = 2.0 - x;&#xA;  else if (x &lt; -1)&#xA;    x = -2.0 - x;&#xA;             &#xA;  if (y &gt; 1)&#xA;    y = 2.0 - y;&#xA;  else if (y &lt; -1)&#xA;    y = -2.0 - y;&#xA;             &#xA;  if (z &gt; 1)&#xA;    z = 2.0 - z;&#xA;  else if (z &lt; -1)&#xA;    z = -2.0 - z;&#xA;              &#xA;//  ; fold sphere onto itself&#xA;         &#xA;  xx=x*x;&#xA;  yy=y*y;&#xA;  zz=z*z;&#xA;  r = xx + yy + zz;&#xA;&#xA; if(r!=0 &amp;&amp; n&gt;4 &amp;&amp; n&lt;100) {&#xA;  red+=xx/r;&#xA;  green+=yy/r;&#xA;  blue+=zz/r;&#xA; }&#xA;&#xA; if (r &gt; gr) { &#xA;  tw = n; break; &#xA; }&#xA;&#xA;  mult=1;&#xA;  if (r &lt; fixedRadius &amp;&amp; r&gt;0) {&#xA;        mult=fixedRadius/r;&#xA;  }&#xA;   mult*=scale;&#xA;   x*=mult;&#xA;   y*=mult;&#xA;   z*=mult;&#xA;   x+=br;&#xA;   y+=bi;&#xA;   z+=bj;&#xA; }&#xA;&#xA;if (invers) {&#xA; if (tw == 0)&#xA;   tw = 1;&#xA; else&#xA;   tw = 0;&#xA;}&#xA;&#xA;if(tw&gt;0) {&#xA;  additionalPointInfo.red=red;&#xA;  additionalPointInfo.green=green;&#xA;  additionalPointInfo.blue=blue;&#xA;}&#xA;&#xA;return (tw);&#xA;&#xA;}&#xA;&#xA;" />

<DD>
not used:<BR>
<BR>
using System;<BR>
using System.Collections.Generic;<BR>
using System.Linq;<BR>
using System.Text;<BR>
using System.Threading.Tasks;<BR>
<BR>
namespace Fractrace.Animation<BR>
{<BR>
<BR>
    public class StepInterpolator<BR>
    {<BR>
<BR>
<BR>
        List&lt;double&gt; hList = new List&lt;double&gt;();<BR>
<BR>
<BR>
        public void StepInterpolator()<BR>
        {<BR>
        }<BR>
<BR>
<BR>
<BR>
<BR>
        public void Add(int t, double h)<BR>
        {<BR>
            if(t&lt;hList.Count)<BR>
                throw new System.ArgumentException(&quot;invalid time parameter&quot;);<BR>
            int lastt=hList.Count-1;<BR>
            double dlastt=lastt;<BR>
            double dt=t;<BR>
            double ddiff=dt-dlastt;<BR>
            double lasth=hList[lastt];<BR>
            double hdiff=(h-lasth)/ddiff;<BR>
            double currentH=lasth;<BR>
            for(int i=0;i&lt;t;++i)<BR>
            {<BR>
                currentH+=hdiff;<BR>
                hList.Add(currentH);<BR>
            }<BR>
            hList.Add(h);<BR>
        }<BR>
<BR>
<BR>
        /// &lt;summary&gt;<BR>
        /// Apply Spline or bezier like interpolation of steps.<BR>
        /// &lt;/summary&gt;<BR>
        public void MakeSmooth()<BR>
        {<BR>
<BR>
        }<BR>
<BR>
<BR>
<BR>
<BR>
        public double Get(int t)<BR>
        {<BR>
            if (hList.Count == 0)<BR>
                return 0;<BR>
            if(t&lt;0)<BR>
                return hList[0];<BR>
            if (t &gt;= hList.Count)<BR>
                return hList[hList.Count - 1];<BR>
            return hList[t];<BR>
        }<BR>
    }<BR>
}<BR>
<BR>
<BR>

</DD>
</DL>
</DD>
</DL>
